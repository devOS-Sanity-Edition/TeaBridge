{
  "schemaVersion": 1,
  "id": "teabridge",
  "version": "${version}",
  "name": "TeaBridge",
  "description": "A simple survival friendly MC -> Discord chat bridge",
  "authors": [
    "devOS: Sanity Edition",
    "Team Nautical"
  ],
  "contributors": [
    "Maximumpower55"
  ],
  "contact": {
    "website": "https://devos.one/",
    "repo": "https://github.com/devOS-Sanity-Edition/TeaBridge"
  },
  "license": "MIT",
  "icon": "assets/teabridge/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      "one.devos.nautical.teabridge.TeaBridge"
    ]
  },
  "mixins": [
    "teabridge.mixins.json"
